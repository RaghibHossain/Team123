#investmentaccount2

#time
age = 22
retire_age = 65
year = 2022
years_to_retirement = retire_age - age

#account
balance = 0
deposit = 20500

#interest
rate = 13.5
real_rate = rate*0.01

retirement_value = 35000000

while balance < retirement_value:
  balance = (balance + deposit)*(1 + real_rate)
  age += 1
print("I will have {} at {} years old.".format(balance, age))
